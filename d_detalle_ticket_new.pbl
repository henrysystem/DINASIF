 retrieve="  SELECT ~"FA_DETVE~".~"ES_CODIGO~",   
         ~"FA_DETVE~".~"VE_NUMERO~",   
         ~"FA_DETVE~".~"IT_CODIGO~",   
         ~"FA_DETVE~".~"EM_CODIGO~",   
         ~"FA_DETVE~".~"SU_CODIGO~",   
         ~"FA_DETVE~".~"BO_CODIGO~",   
         ~"FA_DETVE~".~"DV_SECUE~",   
         ~"FA_DETVE~".~"DV_CANTID~",   
         ~"FA_DETVE~".~"DV_CANDES~",   
         ~"FA_DETVE~".~"DV_DESC1~",   
         ~"FA_DETVE~".~"DV_DESC2~",   
         ~"FA_DETVE~".~"DV_DESC3~",   
         ~"FA_DETVE~".~"DV_DESCUE~",   
         ~"FA_DETVE~".~"DV_TOTAL~",   
		~"FA_DETVE~".~"IM_CODIGO~",
		~"FA_DETVE~".~"DV_VALIVA~",
		~"FA_DETVE~".~"PP_CODIGO~",
         ~"IN_ITEM~".~"IT_CODBAR~",   
         ~"IN_ITEM~".~"IT_CODANT~",   
         ~"IN_ITEM~".~"IT_NOMBRE~",   
         ~"IN_ITEM~".~"MA_CODIGO~",  
          ~"IN_ITEM~".~"IT_STATUS~",  
         ~"IN_ITEM~".~"CO_CODIGO~",   
         ~"IN_ITEM~".~"IT_KIT~",   
         ~"IN_ITEM~".~"IT_COSTO~", 
		0 AS IT_SUCSTOCK,
         ~"IN_ITEM~".~"IT_TIPODESP~", 
         ~"IN_ITEM~".~"IT_TIEMSEC~",  
		~"IN_ITEM~".~"IT_IVA~" ,
         ~"IN_ITEM~".~"IT_VALSTK~",   
   	    ~"IN_ITEM~".~"IT_VALCOSTO~",   
         ~"IN_ITEM~".~"IT_NOMCORTO~",
         ~"IN_ITEM~".~"IT_BASE~",
         ~"IN_ITEM~".~"IT_PREPARADO~",   
         ~"IN_ITEM~".~"UB_CODIGO~",
		~"IN_ITEM~".~"IT_VALCOSTO~",
		~"IN_ITEM~".~"IT_DESC3TARJ~",
         ~"FA_DETVE~".~"DV_ENTREGA~",  
         ~"FA_DETVE~".~"DV_MOTOR~", 
         ~"FA_DETVE~".~"DV_CHASIS~", 
         '                  '~"c_formato~",   
         ~"FA_DETVE~".~"DV_FALTA~",   
         ~"FA_DETVE~".~"DV_PRECIO~",
		~"IN_ITEM~".~"IT_GARANT~",
         ~"IN_ITEM~".~"MR_CODIGO~",
		~"IN_ITEM~".~"IG_CODIGO~",
		~"IN_ITEM~".~"FB_CODIGO~",
		~"IN_ITEM~".~"IT_DESCUE~",
		~"IN_ITEM~".~"IT_DESC3~",		
		~"IN_ITEM~".~"IT_RENTAB~",
		SUBSTR(~"IN_ITEM~".~"CL_CODIGO~",1,2) CL_CODIGO,
		~"IN_ITEM~".~"CL_CODIGO~" CL_CODPROM,
		0 bono,
		0 print, 
		'' PRODPROMO
    FROM ~"FA_DETVE~",   
         ~"IN_ITEM~"  
   WHERE ( ~"FA_DETVE~".~"EM_CODIGO~" = ~"IN_ITEM~".~"EM_CODIGO~" ) and  
         ( ~"FA_DETVE~".~"IT_CODIGO~" = ~"IN_ITEM~".~"IT_CODIGO~" ) and  
         ( ( ~"FA_DETVE~".~"EM_CODIGO~" = :empresa ) AND  
         ( ~"FA_DETVE~".~"SU_CODIGO~" = :sucursal ) AND  
         ( ~"FA_DETVE~".~"ES_CODIGO~" = :estado ) AND  
         ( ~"FA_DETVE~".~"VE_NUMERO~" = :numero) )    
" update="FA_DETVE" updatewhere=0 updatekeyinplace=no arguments=(("empresa", string),("sucursal", string),("estado", string),("numero", number)) )
